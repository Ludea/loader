{
    "name": "blploader",
    "version": "1.0.0",
    "types": "dist/index",
    "main": "dist/index",
    "module": "dist/index",
    "private": true,
    "scripts": {
        "build": "yarn clean && yarn compile",
        "clean": "rimraf dist",
        "compile": "tsc && esbuild Index.tsx --outfile=BLP.js --bundle --minify --loader:.ts=ts",
        "watch": "tsc -w",
        "watch:dev": "yarn watch"
    },
    "devDependencies": {
        "@types/crypto-js": "^4.1.1",
        "rimraf": "5.0.1",
        "typescript": "^5.0.4"
    },
    "dependencies": {
        "crypto-js": "^4.1.1",
        "esbuild": "^0.18.10"
    }
}
